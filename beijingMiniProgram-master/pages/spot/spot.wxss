.log-list {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  position: relative;
  font-size: 28rpx;
}
.inputPosition{
  width: 530rpx;
  height: 60rpx;
  border: 1px solid #ccc;
  margin-left: 30rpx;
  margin-top: 30rpx;
  border-radius: 30rpx;
  background: #f1f1f1;
  padding-left: 30rpx;
}
.myPosition{
  width: 96rpx;
  height: 60rpx;
  position: absolute;
  right: 28rpx;
  top: 30rpx;
}
.myPositionLogo{
  display: block;
  width: 28rpx;
  height: 30rpx;
  color: #999999;
  text-align: center;
  line-height: 30rpx;
  font-size: 30rpx;
  margin-left: 34rpx;
  
}
.myPositionText{
  font-size: 24rpx;
  width: 96rpx;
  height: 30rpx;
  color: #999999;
}
.noEvent{
  width: 100px;
  color: #32b1fa;
  float: left;
}
.countryPosition{
  width: 690rpx;
  height: auto;
  margin-left: 30rpx;
  margin-top: 30rpx;
}
.countryBox{
  width: 690rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-indent: 30rpx;
  border-bottom: 1rpx solid #A1918F;
}
.givCountryPosition{
  width: 738rpx;
  height: auto;
  margin-top: 24rpx;
  margin-left: 7rpx;
}
.givCountryPositionText{
  width: 736rpx;
  height: 50rpx;
  line-height: 50rpx;
  color: #999999;
  line-height: 28rpx;
  display:block;
  text-indent:26rpx;
}
.giveCountryBox{
  width: 200rpx;
  height: 330rpx;
  margin-left: 23rpx;
  margin-right: 23rpx; 
  float: left;
}
.giveCountryNameImg{
  width: 200rpx;
  height: 260rpx;
}
.giveCountryNameText{
  width: 200rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
}
