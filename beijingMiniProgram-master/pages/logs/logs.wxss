.log-list {
  display: flex;
  flex-direction: column;
  font-size: 28rpx;
}
.log-item {
  margin: 10rpx;
}
.dayBoxOne{
  width: 750rpx;
  color: #545454;
}
.dayBoxOneOne{
  width: 750rpx;
  height: 110rpx;
  position: relative;
  color: #545454;
  padding: 20rpx 0 20rpx 30rpx;
}
.dayBoxOneSpan{
  width: 26rpx;
  height: 26rpx;
  border-radius: 50%;
  float: left;
  margin-top: 22rpx;
  margin-left: 3rpx;
  border: 2rpx solid #A6A6A6;
  margin-right: 20rpx;
  background: #ffffff;
}
.dayClick0{
  color: #32b1fa;
}
.dayClick0 .dayBoxOneSpan{
  background: #41acff;
}
.dayBoxOneDay{
  width: 130rpx;
  height: 70rpx;
  float: left;
  margin-right: 23rpx;
}
.dayBoxOneDayDate{
  width: 130rpx;
  height: 35rpx;
  line-height: 34rpx;
  text-align: center;
}
.dayBoxOneDayWeek{
  width: 130rpx;
  height:35rpx;
  line-height: 35rpx;
  text-align: center;
}
.dayBoxOneAm{
  width: 50rpx;
  height: 50rpx;
  float: left;
  border-radius: 50%;
  margin-top: 10rpx;
  font-size: 45rpx;
  text-align: center;
  line-height: 47rpx;
}
.dayBoxOnePm{
  width: 50rpx;
  height: 50rpx;
  float: left;
  border-radius: 50%;
  margin-left: 30rpx;
  margin-top: 10rpx;
  font-size: 45rpx;
  text-align: center;
  line-height: 47rpx;
}
.dayBoxOneWeather{
  width: 330rpx;
  height: 70rpx;
  display: flex;
  align-items: center;
  padding-left: 30rpx;
}
.dayBoxOneAmBottom{
  position: absolute;
  width: 2rpx;
  height: 110rpx;
  left: 44rpx;
  top: 0;
  z-index: -1;
  background: #A6A6A6;
}
.dayBoxOneLine{
  position:absolute;
  width:2rpx;
  height:60rpx;
  left:288rpx;
  top:26rpx;
  background:#A6A6A6;
  transform:rotateZ(25deg);
}
.dayClick0 .dayBoxOneLine{
  background: #32b1fa;
}
.dayBoxOne:nth-child(1) .dayBoxOneAmBottom{
  height: 55rpx;
  top: 55rpx;
}
  .lastOrNo0 .dayBoxOneAmBottom{
  height: 55rpx;
} 
.dayBoxBottombottom .dayBoxOneAmBottom{
  height: 56rpx;
  top: 55rpx;
}
.dayBoxBottomtop .dayBoxOneAmBottom{
  height: 56rpx;
}
.qs-bottom{
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  top: 38rpx;
  font-size: 28rpx;
  line-height: 15px;
  right: 20rpx;  
}
.dayClick0 .qs-bottom{
  transform: rotateZ(180deg);
  top: 44rpx;
} 
.hourBox{
  width: 750rpx;
  background: #f5f5f5;
}
.hourBoxOne{
  width: 750rpx;
  height: 80rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  overflow: hidden;
}
.hourBoxOneTime{
  width: 190rpx;
  height: 40rpx;
  line-height: 40rpx;
  float: left;
  margin-left: 46rpx;
}
.hourBoxOneImg{
  width: 40rpx;
  height: 40rpx;
  margin-right: 40rpx;
  float: left;
  font-size:45rpx;
  text-align:center;
  line-height:37rpx;
}
.hourBoxOneWeather{
  width: 30rpx;
  height: 40rpx;
  line-height: 40rpx;
  float: left;
}
.hourBox2 .hourBoxOne .hourBoxOneWeather{
  width: 60rpx;
}
.hourBox3 .hourBoxOneWeather{
  width: 90rpx;
}
.hourBox4 .hourBoxOneWeather{
  width: 120rpx;
}
.hourBox5 .hourBoxOneWeather{
  width: 150rpx;
}
.hourBoxOneTemp{
  width: 83rpx;
  height: 40rpx;
  line-height: 40rpx;
  float: left;
}
.hourBoxOneWind{
  width: 240rpx;
  height: 40rpx;
  line-height: 40rpx;
  float: left;
}